library(lubridate)
mmwr_to_date <- function(mmwr_year, mmwr_week) {
first_day <- floor_date(make_date(mmwr_year, 1, 4) , unit = "week")
date <- first_day + weeks(mmwr_week - 1)
return(date)
}
